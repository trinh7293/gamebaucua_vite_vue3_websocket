{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-vuetify-ts-starter",
  "description": "Vue2 Vuetify TypeScript Startar project for Vite.",
  "version": "1.2.3",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vue2-vuetify-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vue2-vuetify-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vue2-vuetify-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/vue2-helpers": "^2.1.6",
    "@mdi/font": "^7.2.96",
    "socket.io-client": "^4.6.1",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.14",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.12",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^1.3.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.11.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "1.32.12",
    "stylelint": "^15.5.0",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.0",
    "vite-plugin-checker": "^0.5.6",
    "vitest": "^0.30.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.2.0",
    "vuetify2-component-types": "^2.6.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,toml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
